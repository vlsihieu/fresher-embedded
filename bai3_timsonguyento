#include <stdio.h>
#include<math.h>
#include <stdbool.h>
bool nguuyento(int n);
int timnguyento(int a[], int n);

int timnguyento(int a[],int n)
{
    int b[10]={0};
    int i=0;
    int k=0;
    for (i=0;i<n;i++)
    {
        if ( nguuyento(a[i])==true)
        {
            b[k] = a[i];
            k = k+ 1;
        }
        // if (nguuyento(a[i])==false)
        // {
        //     return -1;
        // }
    }
    for (int i =0;i<k;i++)
    {
        return b[k-1];
    }
    return -1;
}
bool nguuyento(int n)
{
    if (n<2)
    {
        return false;
    }
    else if (n>=2){
        for (int i=2;i<n;i++)
        {
            if (n%i==0)
            {
                return false;
            }
        }
        return true;
    }
}

int main()
{
    // int arr[10] = {1,6,7,8,9,10,11,14,15,67};
    int arr[10] = {6,0,6,8,100,6,14,16,60,20};
    // int k =nguuyento(20);
    // printf("%d\n",k);
    int j = timnguyento(arr,10);
    printf("%d\n",j);
    return 0;
}
